		addAsChild(new BOAttribute<Integer>(this, "ctd_id", false, 0, 0));
		
	}

	@Override
	protected void clearAttributes() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void handleModified(ModifiedEvent source) {
		// TODO Auto-generated method stub
		
	}

}
